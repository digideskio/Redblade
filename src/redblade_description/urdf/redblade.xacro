  	<mass value="1"/>
  	<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
      </inertial>
      <collision>
	<origin rpy="1.5707 0 0" xyz="0 0 0"/>
	<geometry>
	  <cylinder length="0.12" radius="0.1975"/>
	</geometry>
      </collision>
      
